{
  "name": "poc-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=dev npm-run-all --parallel lint server:dev",
    "build": "npm-run-all clean transpile",
    "transpile": "babel ./src --out-dir build --copy-files",
    "clean": "rimraf build",
    "format": "prettier --write \"./**/*.{js,jsx}\"",
    "lint:fix": "eslint . --fix",
    "lint": "`npm bin`/eslint -c .eslintrc.js ./",
    "server:dev": "`npm bin`/nodemon --exec babel-node ./src/bin/www",
    "start": "node ./src/bin/www",
    "watch:lint": "`npm bin`/onchange './**/*.js' -- npm run lint --silent" 
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^4.4.1",
    "knex": "^0.21.15",
    "knex-case-converter-plugin": "^0.1.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  }
}
